<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>ac67d5f4-4690-491a-a0d9-b99948807318</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Project.api\**" />
    <Content Remove="Project.api\**" />
    <EmbeddedResource Remove="Project.api\**" />
    <None Remove="Project.api\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MediatR" Version="12.4.1" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="8.0.10" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.10" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="8.0.10">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="8.0.6" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Project.common\Project.common.csproj" />
    <ProjectReference Include="..\Project.core\Project.core.csproj" />
    <ProjectReference Include="..\Project.dal\Project.dal.csproj" />
  </ItemGroup>

</Project>
